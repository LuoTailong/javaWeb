<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
           version="2.5">

    <servlet>
        <servlet-name>CheckUsernameServlet</servlet-name>
        <servlet-class>web.CheckUsernameServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckUsernameServlet</servlet-name>
        <url-pattern>/checkUsernameServlet</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>CheckNicknameServlet</servlet-name>
        <servlet-class>web.CheckNicknameServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CheckNicknameServlet</servlet-name>
        <url-pattern>/checkNicknameServlet</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>FindCategoryListServlet</servlet-name>
        <servlet-class>web.FindCategoryListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FindCategoryListServlet</servlet-name>
        <url-pattern>/findCategoryListServlet</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>FindPcdListServlet</servlet-name>
        <servlet-class>web.FindPcdListServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FindPcdListServlet</servlet-name>
        <url-pattern>/findPcdListServlet</url-pattern>
    </servlet-mapping>
</web-app>
